
<%= render(
      "flood_risk_engine/enrollments/header",
      title_text: t(".heading.default")
    )
%>
<%= render "flood_risk_engine/enrollments/validation_errors", form: form %>

<%= simple_form_for(
      form,
      as: :address,
      url: enrollment_address_path(form.enrollment, form.address),
      class: "form"
    ) do |f| %>

  <%= render "form", f: f %>

<% end %>

<p>
  <%= link_to(
        t("cancel_go_back"),
        admin_enrollment_exemption_path(form.enrollment_exemption),
        class: "ignore-visited"
      )
  %>
</p>

